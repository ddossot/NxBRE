<?xml version="1.0" encoding="utf-8" standalone="no"?>
<RuleML xmlns="http://www.ruleml.org/0.9/xsd" xsi:schemaLocation="http://www.ruleml.org/0.9/xsd ruleml-0_9-nafdatalog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<oid>
		<Ind>NxBRE Agenda Scheduler Test File</Ind>
	</oid>
  <!--Implications-->
	<Assert mapDirection="forward">
		<Implies>
			<oid>
				<Ind>label:AtoB;priority:99;action:assert</Ind>
			</oid>
			<Atom>
				<Rel>trigger</Rel>
				<Ind>A</Ind>
			</Atom>
			<Atom>
				<Rel>deduction</Rel>
				<Ind>B</Ind>
			</Atom>
		</Implies>
		<Implies>
			<oid>
				<Ind>label:BtoC;priority:90;action:assert</Ind>
			</oid>
			<Atom>
				<Rel>deduction</Rel>
				<Ind>B</Ind>
			</Atom>
			<Atom>
				<Rel>then</Rel>
				<Ind>C</Ind>
			</Atom>
		</Implies>
		<Implies>
			<oid>
				<Ind>label:nafBtoD;priority:80;action:assert</Ind>
			</oid>
			<Naf>
				<Atom>
					<Rel>then</Rel>
					<Ind>C</Ind>
				</Atom>
			</Naf>
			<Atom>
				<Rel>else</Rel>
				<Ind>D</Ind>
			</Atom>
		</Implies>
	</Assert>
	  <!--Facts-->
	<Assert mapDirection="forward">
		<Atom>
			<Rel>trigger</Rel>
			<Ind>A</Ind>
		</Atom>
	</Assert>
</RuleML>
